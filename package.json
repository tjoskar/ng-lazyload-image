{
  "name": "ng2-lazyload-image",
  "version": "2.1.0-beta1",
  "description": "Lazy image loader for Angular 2",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "start": "webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached --hot",
    "build": "typings install && tsc && node make-packages.js",
    "build:example": "webpack --config webpack.prod.config.js --profile --display-error-details --display-cache",
    "e2e": "protractor",
    "e2e:ci": "protractor protractor.ci.conf.js",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "e2e:debug": "protractor --elementExplorer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjoskar/ng2-lazyload-image.git"
  },
  "keywords": [
    "lazy-load",
    "lazy-image",
    "angular2"
  ],
  "author": "Oskar Karlsson <kontakta@oskarkarlsson.nu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjoskar/ng2-lazyload-image/issues"
  },
  "homepage": "https://github.com/tjoskar/ng2-lazyload-image#readme",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "awesome-typescript-loader": "1.0.0",
    "es6-shim": "0.35.0",
    "protractor": "^4.0.0",
    "reflect-metadata": "0.1.6",
    "rxjs": "5.0.0-beta.6",
    "source-map-loader": "^0.1.5",
    "systemjs-builder": "^0.15.9",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "zone.js": "0.6.12"
  },
  "greenkeeper": {
    "ignore": [
      "awesome-typescript-loader"
    ]
  }
}
