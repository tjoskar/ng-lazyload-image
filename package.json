  "dependencies": {},
  "devDependencies": {
    "angular2": "2.0.0-beta.15",
    "awesome-typescript-loader": "^0.17.0-rc.6",
    "es6-shim": "0.35.1",
    "protractor": "^3.1.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs-builder": "^0.15.9",
